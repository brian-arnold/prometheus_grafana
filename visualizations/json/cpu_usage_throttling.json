          "refId": "B"
        },
        {
          "datasource": {
            "uid": "$cluster"
          },
          "expr": "sum by (namespace,pod_name, container_name) ((container_cpu_cfs_throttled_periods_total{container_name=~\"$container\", pod_name=~\"$pod\", namespace=\"$namespace\"} / container_cpu_cfs_periods_total{container_name=~\"$container\", pod_name=~\"$pod\", namespace=\"$namespace\"}) * 100)",
          "legendFormat": "Throttle Percent {{pod_name}} {{container_name}}",
          "refId": "A"
        }
      ],
