      },
      "pluginVersion": "11.6.0",
      "targets": [
        {
          "datasource": {
            "uid": "$cluster"
          },
          "expr": "rate(container_cpu_usage_seconds_total{namespace=~\"$namespace\", pod_name=~\"$pod\", container_name=~\"$container\"}[1m])",
